<body>

    <div class="navbar navbar-fixed-top">
        <div class="navbar-inner">
            <div id="container row">
                <div class="col-xs-6">
                    <a href="@Url.Action("VerifyLogin","Login")" class="brand"><img src="~/Images/TSheader02.png" style="padding-top: 10px;" /></a>
                </div>
                <div class="col-xs-6">



                    <form class="login">

                        @Html.AntiForgeryToken()
                        <div class="col-md-4">
                            <label style="font-size:12px; color:#DFDFDF;">Username</label>
                            @Html.TextBoxFor(m => m.emailAddress, new { @class = "form-control", @style = "width:13em;" })
                        </div>
                        <div class="col-md-8">
                            <label style="font-size:12px; color:#DFDFDF;">Password</label>
                            @Html.PasswordFor(m => m.password, new { @class = "form-control", @style = "width:13em;" })

                            <input type="submit" value="Log in" class="btn btn-xs" style="font-size:10px;" />

                            <input type="button" value="Register" onclick="location.href='@Url.Action("Create","Users")'" style="font-size:10px;" class="btn btn-xs" />


                            <br />
                            <a style="font-size:10px; color:#DFDFDF;" href="@Url.Action("ForgotPasswordSecurityCheck","Login")">Forgot your password?</a>

                        </div>
                    </form> 


                    </div>

                </div>
            </div>
        </div>
